{
  "default_stop_words": [
    "defmodule",
    "defrecord",
    "defexception",
    "defimpl",
    "defprotocol",
    "defstruct",
    "defdelegate",
    "def.+(.+\\\\.+).+do",
    "^\\s+use\\s+"
  ],
  "custom_stop_words": [
    "{ :ok, .+ } <-"
  ],
  "coverage_options": {
    "treat_no_relevant_lines_as_covered": true
  },
  "skip_files": [
    "lib/cachex/stats.ex"
  ]
}
